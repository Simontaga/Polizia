// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Event {
  dbID Int @id @default(autoincrement())
  id  Int
  datetime DateTime
  name String
  summary String
  url String
  type String
  locationName String
  locationGps String
}